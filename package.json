{
  "name": "clementine-classic-shop",
  "version": "1.0.0",
  "description": "E-commerce fashion platform",
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run install:all && npm run build:frontend && npm run build:backend",
    "start": "cd backend && NODE_ENV=production npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["ecommerce", "fashion"],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "20.11.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
